using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter number of days:");
        int days = int.Parse(Console.ReadLine());

        int years = days / 365;
        days %= 365;
        int months = days / 30;
        days %= 30;

        Console.WriteLine($"{years} years, {months} months, {days} days");
    }
}
